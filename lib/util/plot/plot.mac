init() := block(
	[
		log : get_logger("plot", log_level_info)
	],
	
	devnull : buildq([log],
		lambda([sol],
			log@info("plot > /dev/null")
		)
	),

	console : buildq([log],
		lambda([sol],
			block(
				log@info("Solution:"),
				for p in sol do log@info(p),
				log@info("End of solution.")
			)
		)
	)
) $